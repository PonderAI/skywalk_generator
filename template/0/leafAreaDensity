FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      leafAreaDensity;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField

{
    #includeEtc "caseDicts/setConstraintTypes"
    "(ground|terrain)"
    {
        type            zeroGradient;
    }
    "proc.*"
    {
    type            processor;
    }
    "(inlet|surroundings|building_1|building_2|skywalk|top|sides|outlet)"
    {
        type            fixedValue;
        value           uniform 0;
    }

   

}


